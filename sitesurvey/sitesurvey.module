/**
 * Implements hook_permission().
 */
function sitesurvey_permission() {
  $permissions = array(
    'respond to site survey' => array(
      'title' => t('Respond to the Site Survey'),
    ),
    'view site survey results' => array(
      'title' => t('View Site Survey results'),
    ),
  );
  return $permissions;
}
